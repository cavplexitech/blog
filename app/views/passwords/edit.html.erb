<h1>Edit Password</h1>
<%= form_with model: Current.user, url: edit_password_path do |form| %>
  <p>
  <%= form.label 'New Password:'%><br>
  <%= form.password_field :password %>
  </p>

  <p>
  <%= form.label 'Confirm Password' %><br>
  <%= form.password_field :password_confirmation %>
  </p>

  <p>
  <%= form.submit 'Update' %>
  </p>
<% end %>




